    "js-cookie": "^2.2.0",
    "lodash": "latest",
    "next": "latest",
    "next-compose-plugins": "latest",
    "next-cookies": "latest",
    "next-images": "latest",
    "next-progressbar": "latest",
    "next-routes": "latest",
    "node-sass": "latest",
    "react": "latest",
    "react-dom": "latest",
